{
    "active_detection": {
        "enabled": true,
        "interval": 2,
        "snapshot_url": "http://localhost:5000/snapshot",
        "use_local_camera": true
    },
    "camera": {
        "initialize": true,
        "options": {
            "height": 480,
            "mime": "image/jepg",
            "port": 8080,
            "width": 640
        }
    },
    "commands": {
        "get_current_detection": "/detect",
        "get_help": "/",
        "get_snapshot": "/snapshot",
        "get_start_active_detection": "/start_detect",
        "get_stop_active_detection": "/stop_detect",
        "post_detect_from_image_file": "/file"
    },
    "human_params": {
        "async": false,
        "backend": "tensorflow",
        "body": {
            "enabled": true,
            "maxDetected": 1,
            "minConfidence": 0.2,
            "skipFrames": 1
        },
        "cacheSensitivity": 0.75,
        "debug": true,
        "face": {
            "antispoof": {
                "enabled": false,
                "skipFrames": 14
            },
            "description": {
                "enabled": true,
                "minConfidence": 0.1,
                "skipFrames": 11
            },
            "detector": {
                "iouThreshold": 0.1,
                "maxDetected": 15,
                "minConfidence": 0.2,
                "return": false,
                "rotation": false,
                "skipFrames": 15
            },
            "emotion": {
                "enabled": true,
                "minConfidence": 0.1,
                "skipFrames": 17
            },
            "enabled": true,
            "iris": {
                "enabled": true
            },
            "mesh": {
                "enabled": true
            }
        },
        "filter": {
            "blur": 0,
            "brightness": 0,
            "contrast": 0,
            "enabled": false,
            "flip": false,
            "height": 0,
            "hue": 0,
            "kodachrome": false,
            "negative": false,
            "pixelate": 0,
            "polaroid": false,
            "return": true,
            "saturation": 0,
            "sepia": false,
            "sharpness": 0,
            "technicolor": false,
            "vintage": false,
            "width": 0
        },
        "gesture": {
            "enabled": true
        },
        "hand": {
            "detector": {
                "modelPath": "file://modules/human/models/handtrack.json"
            },
            "enabled": false,
            "iouThreshold": 0.2,
            "landmarks": true,
            "maxDetected": 1,
            "minConfidence": 0.8,
            "rotation": true,
            "skipFrames": 18
        },
        "modelBasePath": "file://modules/human/models/",
        "object": {
            "enabled": false,
            "iouThreshold": 0.4,
            "maxDetected": 10,
            "minConfidence": 0.2,
            "skipFrames": 19
        },
        "segmentation": {
            "blur": 8,
            "enabled": false
        },
        "skipFrame": false
    },
    "log_prefix": "[human-cv]",
    "port": 3000
}
